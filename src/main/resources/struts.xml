<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>


	<constant name="struts.dispatcher.parametersWorkaround" value="false" />

	<package name="basePackage" extends="struts-default">
		<action name="duty" class="com.iTree.action.DutyAction" method="doEdit">
			<result name="success" type="redirect">/backend/duty/dutyEdit.jsp
			</result>
			<result name="false" type="redirect">/backend/duty/dutyEdit.jsp</result>
		</action>
		<action name="duty2" class="com.iTree.action.DutyAction">
			<result name="save_success" type="redirect">a.jsp</result>
			<result name="update_success" type="redirect">a.jsp</result>
			<result name="delete_success" type="redirect">a.jsp</result>
		</action>

	</package>

</struts>